{
  "IDX.aI.enableInlineCompletion": true,
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": "explicit"
    }
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "prettier.documentSelectors": [
    "**/*.astro"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
